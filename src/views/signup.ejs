      <br>
      <span id="exampleInputPassword1err" class="text-danger font-weight-bold"></span>
      <br>
      <div class="form-group ">
        <label for="pwdid">Confirm Password:</label>
        <br>
        <input type="password" name="pwdid" class="form-control-md-4" id="pwdid" placeholder="Enter password" >
        <br>
        <span id="pwdiderr" class="text-danger font-weight-bold"></span>
